{
  "name": "auth0-tenant-reset",
  "version": "1.0.0",
  "description": "An Auth0 CLI tool/extension that can be used to reset an Auth0 tenant to a known set of artifacts.",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && babel src -d dist",
    "start": "./bin/tenant-reset",
    "install": "npm run build"
  },
  "bin": {
    "tenant-reset": "./bin/tenant-reset"
  },
  "repository": "https://github.com/auth0-extensions/auth0-tenant-reset",
  "author": "twistedstream",
  "license": "MIT",
  "dependencies": {
    "auth0": "^2.3.1",
    "babel-cli": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "colors": "^1.1.2",
    "dotenv": "^2.0.0",
    "request": "^2.74.0",
    "rimraf": "^2.5.4"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.3.1",
    "eslint-plugin-import": "^1.13.0"
  }
}
